.button-link:focus-visible::before,
.form-upload-dropzone label span:focus-visible::before,
.dropdown-link:focus-visible::before,
button:focus-visible,
*[tabindex="0"]:focus-visible {
  -webkit-box-shadow: 0 0 0 4px #ffffff, 0 0 0 8px rgba(0, 150, 209, 0.15);
          box-shadow: 0 0 0 4px #ffffff, 0 0 0 8px rgba(0, 150, 209, 0.15);
  outline: 2px solid #009fdf;
  outline-offset: 0;
}

a:focus-visible > img, .button-link:focus-visible > img, .form-upload-dropzone label span:focus-visible > img, .dropdown-link:focus-visible > img,
a:focus-visible::before > img,
