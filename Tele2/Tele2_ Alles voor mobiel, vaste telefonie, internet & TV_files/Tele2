  background-color: transparent;
  border-radius: 100px;
  border-width: 0;
  display: inline-block;
  font-family: "Tele2SlabWeb", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 1rem;
  font-weight: 700;
  height: 48px;
  text-align: center;
  text-decoration: none;
  text-transform: none;
  -webkit-transition: background-color 0.3s ease-in-out, color 0.3s ease-in-out;
  -o-transition: background-color 0.3s ease-in-out, color 0.3s ease-in-out;
  transition: background-color 0.3s ease-in-out, color 0.3s ease-in-out;
  -webkit-user-select: none;
     -moz-user-select: none;
      -ms-user-select: none;
          user-select: none;
  vertical-align: baseline;
  white-space: nowrap;
}

.button-blue:hover, .button-light-blue:hover, .button-purple:hover, .button-magenta:hover, .button-red:hover, .button-orange:hover, .button-yellow:hover, .button-green:hover, .button-light-green:hover, .button-black:hover, .snackbar-action:hover, .snackbar-action-secondary:hover, .button-outline-blue:hover, .button-outline-light-blue:hover, .button-outline-purple:hover, .button-outline-magenta:hover, .button-outline-red:hover, .button-outline-orange:hover, .button-outline-yellow:hover, .button-outline-green:hover, .button-outline-light-green:hover, .button-outline-white:hover, .button-outline-black:hover {
  text-decoration: none;
}

.button-blue:not(:disabled), .button-light-blue:not(:disabled), .button-purple:not(:disabled), .button-magenta:not(:disabled), .button-red:not(:disabled), .button-orange:not(:disabled), .button-yellow:not(:disabled), .button-green:not(:disabled), .button-light-green:not(:disabled), .button-black:not(:disabled), .snackbar-action:not(:disabled), .snackbar-action-secondary:not(:disabled), .button-outline-blue:not(:disabled), .button-outline-light-blue:not(:disabled), .button-outline-purple:not(:disabled), .button-outline-magenta:not(:disabled), .button-outline-red:not(:disabled), .button-outline-orange:not(:disabled), .button-outline-yellow:not(:disabled), .button-outline-green:not(:disabled), .button-outline-light-green:not(:disabled), .button-outline-white:not(:disabled), .button-outline-black:not(:disabled) {
  cursor: pointer;
}

.button-blue:disabled, .button-light-blue:disabled, .button-purple:disabled, .button-magenta:disabled, .button-red:disabled, .button-orange:disabled, .button-yellow:disabled, .button-green:disabled, .button-light-green:disabled, .button-black:disabled, .snackbar-action:disabled, .snackbar-action-secondary:disabled, .button-outline-blue:disabled, .button-outline-light-blue:disabled, .button-outline-purple:disabled, .button-outline-magenta:disabled, .button-outline-red:disabled, .button-outline-orange:disabled, .button-outline-yellow:disabled, .button-outline-green:disabled, .button-outline-light-green:disabled, .button-outline-white:disabled, .button-outline-black:disabled {
  cursor: not-allowed;
}

[class^="button-"] [class^="icon-"] {
  font-size: 1.25rem;
}

[class^="button-"] [class^="icon-"]:first-child:not(:last-child) {
  margin: 0 0.4em 0 0;
}

[class^="button-"] [class^="icon-"]:last-child:not(:first-child) {
  margin: 0 0 0 0.4em;
}

