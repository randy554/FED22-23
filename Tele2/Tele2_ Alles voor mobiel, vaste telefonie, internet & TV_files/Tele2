  .table-compare tbody tr.table-compare-label-header + tr > td {
    border-top: none;
  }
  .table-compare tbody tr.table-compare-label-header + tr > td:first-of-type::after, .table-compare tbody tr.table-compare-label-header + tr > td:last-of-type::after {
    display: none;
  }
}

